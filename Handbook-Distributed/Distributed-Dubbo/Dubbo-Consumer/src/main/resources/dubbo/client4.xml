<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd
         http://code.alibabatech.com/schema/dubbo
         http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--提供方信息-->
    <dubbo:application name="client-xml-one" owner="LYS"/>
    <!--zookeeper注册中心-->
    <dubbo:registry protocol="zookeeper" address="192.168.1.165:2181,192.168.1.131:2181" />

    <dubbo:reference id="serviceOne" interface="com.handbook.dubbo.api.ServiceOne" protocol="dubbo"
                     version="1.0.0"  check="false" timeout="30" mock="com.handbook.dubbo.mock.ServiceOneMock"/>

</beans>